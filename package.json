{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application powered by React",
  "main": "./src/main/index.js",
  "scripts": {
    "start": "electron ./src/main/index.js --app-log --app-log-level=trace | npx pino-pretty",
    "server": "electron ./src/main/index.js --app-log --app-log-level=trace --server-mode | npx pino-pretty",
    "build": "webpack --mode production",
    "dev-server": "webpack-dev-server --open --mode development",
    "electron-build": "electron-builder",
    "clean": "rm -rf dist",
    "test": "mocha"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.1",
    "devtron": "^1.4.0",
    "electron": "^18.3.7",
    "electron-builder": "^22.4.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-mocha": "^6.3.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "mocha": "^7.1.1",
    "pino-pretty": "^4.0.0",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "@types/pino": "^5.17.0",
    "axios": "^0.19.2",
    "cron": "^1.8.2",
    "dotenv": "^8.2.0",
    "electron-is-dev": "^1.2.0",
    "electron-store": "^5.1.1",
    "electron-unhandled": "^3.0.2",
    "fastify": "^2.13.1",
    "fastify-static": "^2.6.0",
    "p-queue": "^6.4.0",
    "pino": "^6.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "rss-parser": "^3.7.6",
    "throttle-debounce": "^2.1.0",
    "uniqid": "^5.2.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "build": {
    "productName": "ElectronReactApp",
    "appId": "raoul2000.electron.react.app",
    "win": {
      "target": [
        "portable"
      ]
    }
  }
}
